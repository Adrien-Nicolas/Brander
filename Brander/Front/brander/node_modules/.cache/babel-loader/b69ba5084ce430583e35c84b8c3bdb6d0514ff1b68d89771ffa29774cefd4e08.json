 border: 1px solid #333;\n      height: 2.5em;\n      padding: 0.5em;\n      transition: height 400ms cubic-bezier(0.23, 1, 0.32, 1);\n      padding:0 50px;\n    }\n    .menu > li {\n      display: flex;\n      justify-content: center;\n      margin: 0;\n      padding: 0.5em 0;\n      width: 100%;\n      color: white;\n      background-color: #222;\n    }\n    .menu > li:not(:last-child) {\n      border-bottom: 1px solid #444;\n    }\n  \n    .logo img{\n    display: none;\n    }\n  \n    .explanation{\n      position: absolute;\n      top: 70%;\n      left: 50%;\n      transform: translate(-50%, -50%);\n  \n    }\n  \n  }\n  \n  @media (max-width: 1515px) {\n    .logo img{\n    display: none;\n    }\n  \n    .explanation{\n      position: absolute;\n      top: 60%;\n      left: 50%;\n      transform: translate(-50%, -50%);\n      text-align: center;\n  \n  \n  \n    }\n  }\n  \n  @media (min-width: 1900px) {\n    /**Put the menu on the right */\n    .top-nav {\n      top: 1%;\n      right: 11%;\n    }\n    .menu {\n      margin-right: 0;\n    }\n    .menu > li {\n      margin: 0 ;\n    }\n  }\n  \n  \n  </style>"],"mappings":"AAmCE,eAAe,CAEb"},"metadata":{},"sourceType":"module","externalDependencies":[]}
olor: #EEF4ED;\n          height: 1080px;\n          max-height: 900px;\n  }\n  \n  .title{\n    position: absolute;\n    top: 30%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    text-align: center;\n    font-size: 40px;\n    color: rgb(0, 0, 0);\n    font-family: 'Auxhie';\n    /**Put in z index 0 */\n    z-index: 0;\n  }\n  \n  .logo img{\n    position: absolute;\n    top: 70%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    width: 300px;\n    height: 300px;\n    border-radius: 10%;\n  }\n  \n  .explanation{\n    position: absolute;\n    top: 50%;\n    left: 80%;\n    transform: translate(-50%, -50%);\n    text-align: center;\n    font-size: 20px;\n    color: rgb(255, 255, 255);\n    font-family: 'Auxhie';\n  \n  }\n  \n  .explanation h1{\n    font-size: 70px;\n  }\n  \n  .explanation p{\n    font-size: 30px;\n  }\n  \n  button{\n  align-items: flex-start;\n  background-color: rgb(255, 255, 255);\n  border: none;\n  color: rgb(0, 0, 0);\n  padding: 15px 32px;\n  text-align: center;\n  text-decoration: none;\n  display: inline-block;\n  font-size: 16px;\n  border-radius: 10px;\n  cursor: pointer;\n  font-family: 'Auxhie';\n  \n  }\n  \n  /**button hover */\n  button:hover{\n    background-color: rgb(0, 0, 0);\n    color: rgb(255, 255, 255);\n    transition: 0.5s;\n    /**get bigger, and lower when mouse is not on the butoon */\n    transform: scale(1.1);\n  }\n  \n  \n  \n  .top-nav {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: nowrap;\n    justify-content: space-around;\n    align-items: baseline;\n    align-content: stretch;\n    color: #FFF;\n    height: 50px;\n    padding: 1em;\n    margin-left: 2%;\n    position: absolute;\n    top: 2%;\n    \n    \n  \n    \n  }\n  \n  .menu {\n    display: flex;\n    flex-direction: row;\n    list-style-type: none;\n    margin: 0;\n    padding: 0;\n    margin-right: 20px;\n    z-index: 1;\n    position: fixed;\n    white-space: nowrap;\n    /**keep the same size when zoom and dezoom
rder-radius: 10px;\n  cursor: pointer;\n  font-family: 'Auxhie';\n\n}\n\n/**button hover */\nbutton:hover{\n  background-color: rgb(0, 0, 0);\n  color: rgb(255, 255, 255);\n  transition: 0.5s;\n  /**get bigger, and lower when mouse is not on the butoon */\n  transform: scale(1.1);\n}\n\n\n.contact_title{\n  font-family: 'Auxhie';\n  position: absolute;\n  top: 5%;\n  right: 15%;\n  color: #EEF4ED;\n}\n\n\n\n\n\n    .logo img{\n    position: absolute;\n    width: 250px;\n    height: 250px;\n    border-radius: 10%;\n    right: 76%;\n    top: 30%;\n    \n  }\n\n\n  \n  .background{\n      filter: blur(0px);\n      /* Full height */\n      /* Center and scale the image nicely */\n      background-position: center;\n      background-repeat: no-repeat;\n      background-size: cover;\n      display: flex;\n      flex-direction: row;\n      justify-content: center;\n      align-items: center;\n      padding: 50px;\n      background-color:#231F20;\n      color: #EEF4ED;\n      height: 1080px;\n      max-height: 900px;\n      border-radius: 25px;\n\n\n  }\n  \n\n  h1 {\n    font-size: 3rem;\n    font-weight: bold;\n    margin-bottom: 1rem;\n    color: #231F20;\n    position: absolute;\n    top: 60%;\n    left: 11%;\n  }\n  form {\n    margin-bottom: 1rem;\n    margin-left: 700px ;\n    padding: 15px;\n  }\n  label { \n    display: block; \n    margin-bottom: 4px; \n    font-weight: bold; \n    font-size: 0.9rem;\n  }\n   textarea, select {\n    border: 1px solid #ccc;\n    font-size: 1rem;\n    padding: 6px 10px;\n    border-radius: 4px;\n    max-width: 500px;\n    max-height:200px;\n  }\n  body { display: block }\n  \n  button { \n    background-color: rgb(205, 205, 205);\n    border: none;\n    color: rgb(0, 0, 0);\n    padding: 15px 10px;\n    text-align: center;\n    text-decoration: none;\n    display: inline-block;\n    font-size: 16px;\n    border-radius: 10px;\n    cursor: pointer;\n    margin-left: 700px ;\n    \n  }\n  \n  form{\n    position: absolute;\n    top: 30%;\n    right: 19%;\n  }\n\n  @media (max-width: 1280px) {\n    .contact_title{\n      display: none;\n    }\n    .explanation{\n    display: none;\n    }\n\n    .title{\n      display: none;\n  \n    }\n    .background{\n      background-image: none;\n    }\n    .logo img{\n      position: absolute;\n      top: 10%;\n      right: 38%;\n    }\n\n    form{\n      position: absolute;\n      top: 40%;\n      right: 36%;\n    }\n\n  }\n\n\n  @media (max-width: 768px) {\n\n    .logo img{\n      position: absolute;\n      top: 10%;\n      right: 30%;\n    }\n\n    form{\n      position: absolute;\n      top: 40%;\n      right: 27%;\n    }\n\n\n  }\n\n\n  @media (max-width: 425px) {\n\n.logo img{\n  position: absolute;\n  top: 10%;\n  right: 19%;\n}\n\nform{\n  position: absolute;\n  top: 40%;\n  right: 14%;\n}\n\n}\n\n\n\n@media (max-width: 320px) {\n\n.logo img{\n  position: absolute;\n  top: 10%;\n  right: 10%;\n}\n\nform{\n  position: absolute;\n  top: 40%;\n  right: 4%;\n}\n\n}\n\n\n  \n  </style>"],"mappings":"AAoCE,eAAe,CAEb"},"metadata":{},"sourceType":"module","externalDependencies":[]}
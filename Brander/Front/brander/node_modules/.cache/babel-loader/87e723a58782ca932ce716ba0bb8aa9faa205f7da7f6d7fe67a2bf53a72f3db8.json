{"ast":null,"code":"import jsonData from '../../project.json';\nexport default {\n  data() {\n    return {\n      projects: jsonData.ProjectsArray,\n      categoryFilter: null\n    };\n  },\n  computed: {\n    filteredProjects() {\n      if (!this.categoryFilter) {\n        return this.projects;\n      }\n      return this.projects.filter(project => project.Category === this.categoryFilter);\n    }\n  },\n  methods: {\n    filterProjects(category) {\n      this.categoryFilter = category;\n    }\n  }\n};","map":{"version":3,"names":["jsonData","data","projects","ProjectsArray","categoryFilter","computed","filteredProjects","filter","project","Category","methods","filterProjects","category"],"sources":["/home/adrien/Documents/perso/perso/Brander/Front/brander/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div id=\"Arts\">\n    <div class=\"background\">\n      <br>\n      <section class=\"top-nav-prod\">\n        <input id=\"menu-toggle-prod\" type=\"checkbox\" />\n        <label class='menu-button-container' for=\"menu-toggle-prod\">\n          <div class='menu-button'></div>\n        </label>\n        <ul class=\"menu\">\n          <li class=\"Music\" @click=\"filterProjects('Music')\">Music</li>\n          <li class=\"Artwork\" @click=\"filterProjects('Artwork')\">Artwork</li>\n          <li class=\"Design\" @click=\"filterProjects('Design')\">Design</li>\n        </ul>\n      </section>\n      <div class=\"main-container2\" ref=\"main-container2\">\n        <div class=\"project-card\" v-for=\"item in filteredProjects\" :key=\"item.id\">\n          <h2>{{ item.Title }}</h2>\n          <div class=\"img-div\">\n            <img class=\"project-img\" :src=\"require('../assets/projects/' + item.Image)\" alt=\"\">\n          </div>\n          <p>{{ item.About }}</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport jsonData from '../../project.json';\n\nexport default {\n  data() {\n    return {\n      projects: jsonData.ProjectsArray,\n      categoryFilter: null,\n    };\n  },\n  computed: {\n    filteredProjects() {\n      if (!this.categoryFilter) {\n        return this.projects;\n      }\n      return this.projects.filter((project) => project.Category === this.categoryFilter);\n    },\n  },\n\n  methods: {\n    filterProjects(category) {\n      this.categoryFilter = category;\n    },\n  },\n};\n</script>\n  \n  <style scoped>\n  \n  @font-face {\n    font-family: \"Auxhie\";\n    src: local(\"Auxhie\"),\n    url(../font/auxhie-modern-sport-font/OpenType-TT/Auxhie-Regular.ttf) format(\"truetype\");\n    }\n\n  .background{\n      filter: blur(0px);\n\n      /* background-image: url('../assets/prod_bg.png'); */\n      background-position: center;\n      background-repeat: no-repeat;\n      background-size: cover;\n      display: flex;\n      flex-direction: row;\n      justify-content: center;\n      align-items: center;\n      padding: 50px;\n      background-color: #231F20;\n      color: #EEF4ED;\n  }\n  \n  .menu > li:hover{\n  transition: 0.2s;\n  /**get bigger, and lower when mouse is not on the butoon */\n  transform: scale(1.15);\n}\n\n  p{\n        color: rgb(255, 254, 254);\n        font-size: 1.1em;\n        font-family: 'Auxhie';\n\n        text-align: justify;\n        text-justify: auto;\n        text-align: center;\n        line-height: 1.4em;\n        margin: 20px;\n        border-radius: 2%;\n    }\n\n    h1{\n        margin-top: 5%;\n        margin-bottom: 50px;\n        color: aliceblue;\n        font-family: 'Auxhie';\n\n    }\n\n    .main-container2{\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      flex-wrap: wrap;\n      padding: 10%;\n      margin: 0 auto; \n}\n\n.project-card{\n  min-width: 150px;\n  max-width: 30%;\n  margin: 20px;\n  padding: 10px;\n  background-color: #413f3f; \n  border-radius: 25px; \n  \n}\n\n    .project-card:hover{\n      transform: scale(1.05);\n      transition: all 0.2s ease-in-out;\n    }\n  .project-card img{\n        border-radius: 25px;\n        height: 10rem;\n        filter: grayscale(100%);\n        transition: all 0.7s ease-in-out;\n        padding: 10px;\n    }\n\n\n    .project-card:hover img{\n        filter: grayscale(0%);\n        transform: scale(1.2);\n        transition: all 0.2s ease-in-out;\n    }\n\n\n    .project-img{\n        border-radius: 25px;\n        height: 5rem;\n    \n    }\n\n    .img-div{\n        display: flex;\n        justify-content: center;\n        align-items: center; \n        \n    }\n  \n\n    .top-nav-prod {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: nowrap;\n  justify-content: space-around;\n  align-items: baseline;\n  align-content: stretch;\n  color: #FFF;\n  height: 50px;\n  padding: 1em;\n  margin-left: 2%;\n  position: absolute;\n  top: 0%;\n    \n}\n\n.menu {\n  display: flex;\n  flex-direction: row;\n  list-style-type: none;\n  margin: 0;\n  padding: 0;\n  margin-right: 20px;\n  z-index: 1;\n  position: fixed;\n  white-space: nowrap;\n  top: 3%;\n  /**keep the same size when zoom and dezoom*/\n  \n\n}\n\n.menu > li {\n  text-align: center;\n  padding: 20px ;\n  font-size: 30px;\n  color: rgb(255, 255, 255);\n  font-family: 'Abhaya Libre ExtraBold', sans-serif;\n  cursor: pointer;\n  list-style-type: none;\n  overflow: hidden;\n  /**add space between LI */\n  margin: 0 7em;\n\n  \n}\n\n.menu-button-container {\n  display: none;\n  height: 100%;\n  width: 30px;\n  cursor: pointer;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n#menu-toggle-prod {\n  display: none;\n}\n\n.menu-button,\n.menu-button::before,\n.menu-button::after {\n  display: block;\n  background-color: #fff;\n  position: absolute;\n  height: 4px;\n  width: 30px;\n  transition: transform 400ms cubic-bezier(0.23, 1, 0.32, 1);\n  border-radius: 2px;\n}\n\n.menu-button::before {\n  content: '';\n  margin-top: -8px;\n}\n\n.menu-button::after {\n  content: '';\n  margin-top: 8px;\n}\n\n#menu-toggle-prod:checked + .menu-button-container .menu-button::before {\n  margin-top: 0px;\n  transform: rotate(405deg);\n}\n\n#menu-toggle-prod:checked + .menu-button-container .menu-button {\n  background: rgba(255, 255, 255, 0);\n}\n\n#menu-toggle-prod:checked + .menu-button-container .menu-button::after {\n  margin-top: 0px;\n  transform: rotate(-405deg);\n}\n\n  \n@media (max-width: 1448px) {\n  .menu-button-container {\n    display: flex;\n  }\n  .menu {\n    position: absolute;\n    top: 0;\n    margin-top: 50px;\n    left: 0;\n    flex-direction: column;\n    width: 100%;\n    justify-content: center;\n    align-items: center;\n  }\n  #menu-toggle-prod ~ .menu li {\n    height: 0;\n    margin: 0;\n    padding: 0;\n    border: 0;\n    transition: height 400ms cubic-bezier(0.23, 1, 0.32, 1);\n  }\n  #menu-toggle-prod:checked ~ .menu li {\n    border: 1px solid #333;\n    height: 2.5em;\n    padding: 0.5em;\n    transition: height 400ms cubic-bezier(0.23, 1, 0.32, 1);\n    padding:0 50px;\n  }\n  .menu > li {\n    display: flex;\n    justify-content: center;\n    margin: 0;\n    padding: 0.5em 0;\n    width: 100%;\n    color: white;\n    background-color: #222;\n  }\n  .menu > li:not(:last-child) {\n    border-bottom: 1px solid #444;\n  }\n\n\n}\n  </style>"],"mappings":"AA6BA,OAAOA,QAAO,MAAO,oBAAoB;AAEzC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAEF,QAAQ,CAACG,aAAa;MAChCC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAAC,IAAI,CAACF,cAAc,EAAE;QACxB,OAAO,IAAI,CAACF,QAAQ;MACtB;MACA,OAAO,IAAI,CAACA,QAAQ,CAACK,MAAM,CAAEC,OAAO,IAAKA,OAAO,CAACC,QAAO,KAAM,IAAI,CAACL,cAAc,CAAC;IACpF;EACF,CAAC;EAEDM,OAAO,EAAE;IACPC,cAAcA,CAACC,QAAQ,EAAE;MACvB,IAAI,CAACR,cAAa,GAAIQ,QAAQ;IAChC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}